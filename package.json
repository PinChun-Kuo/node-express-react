{
  "name": "node-express-react",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "set NODE_ENV=production&& webpack -p --config webpack.config.js --progress --profile --colors && npm run start",
    "dev": "set NODE_ENV=development&& webpack-dev-server --devtool eval --progress --colors --content-base build",
    "test-win-karma": "./node_modules/.bin/karma start",
    "test-win": "set NODE_ENV=development&& npm run test-win-karma",
    "test-linux": "export NODE_ENV=development&& node_modules/.bin/karma start"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "^2.5.2",
    "express": "~4.14.0",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "node-sass": "^3.13.0",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.6",
    "react-bootstrap-table": "^2.5.8",
    "react-dom": "^15.3.2",
    "react-modal": "^1.5.2",
    "sass-loader": "^4.0.2",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.0",
    "eslint": "^3.10.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-react": "^6.6.0",
    "html-webpack-plugin": "^2.24.1",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.1.2",
    "react": "^0.13.3",
    "react-addons-test-utils": "^15.3.2",
    "react-bootstrap": "^0.30.6",
    "react-hot-loader": "^1.3.0",
    "should": "^11.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }

{
  "name": "node-express-react",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "export NODE_ENV=production&& export PORT=3000&& export HOST=127.0.0.1&& webpack -p --config webpack.config.js --progress --profile --colors&& node_modules/.bin/babel-node app.js",
    "dev-backend": "export NODE_ENV=development&& export PORT=3000&& export HOST=127.0.0.1&& babel-node app.js",
    "dev-win": "set NODE_ENV=development&& set PORT=3000&& set HOST=127.0.0.1&& webpack-dev-server --devtool eval --progress --colors --content-base build",
    "dev-linux": "export NODE_ENV=development&& export PORT=3000&& export HOST=127.0.0.1&& webpack-dev-server --devtool eval --progress --colors --content-base build",
    "test-backend": "export NODE_ENV=development&& export HOST=127.0.0.1&& mocha --compilers js:babel-register ./test/libs",
    "test-win-karma": "./node_modules/.bin/karma start",
    "test-win": "set NODE_ENV=development&& npm run test-win-karma",
    "test-linux": "export NODE_ENV=development&& node_modules/.bin/karma start",
    "lint-backend": "eslint app.js ./libs ./models ./routes/issue.js",
    "lint-test-backend": "eslint ./test/libs"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "^2.5.2",
    "express": "~4.14.0",
    "immutable": "^3.8.1",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "node-sass": "^3.13.0",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-actions": "^1.1.0",
    "redux-immutable": "^3.0.8",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.2",
    "sequelize": "^3.27.0",
    "serve-favicon": "~2.3.0",
    "sync": "^0.2.5"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
